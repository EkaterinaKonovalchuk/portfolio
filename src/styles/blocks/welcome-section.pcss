.welcome-section{
  background: url(../images/content/welcome-section.png) center center/cover no-repeat;
  clip-path: polygon(0 0, 0 93%, 50% 100%, 100% 93%, 100% 0 );

  @include tablets{
    clip-path: polygon(0 0,0 97%,50% 100%,100% 97%,100% 0);
  }

  @include phones{
    padding-bottom:100px;
  }
}
